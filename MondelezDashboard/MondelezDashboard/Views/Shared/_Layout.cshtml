<!DOCTYPE html>
<html>
<head>
    <link rel="shortcut icon" href="~/Content/favicon.ico" type="image/x-icon" sizes="16x16" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=11">
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/Montserrat.css" rel='stylesheet'/>
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/Oswald.css" rel='stylesheet'/>
    <script src="~/Scripts/Plugin/require.js" data-main="../Scripts/RequireJsConf"></script>
    <base href="/Home/Index#!/">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id='@ViewBag.GID'"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());
    gtag('config', '@ViewBag.GID', { 'page_path': 'MondelezPBS/'});
    </script>
</head>
<body id="body1">
    <script type="text/javascript">
        sessionStorage['LoginPage'] = '@ViewBag.LoginPage';
        sessionStorage['LoginPageURL'] = '@ViewBag.LoginPageURL';
        sessionStorage['IsSSo'] = '@ViewBag.IsSSO';
        sessionStorage['GID'] = '@ViewBag.GID';
    </script>
    @RenderBody()
</body>
</html>
